<!-- Sample -->
<!--*************************************************-->



<xsd:complexType name="soilType2">
	<xsd:sequence>
		<xsd:element name="OrganicLayer" type="organicLayerType" minOccurs="0" maxOccurs="unbounded"/>
		<xsd:element name="MineralSoil" type="mineralSoilType" minOccurs="0"  maxOccurs="unbounded"/>
	</xsd:sequence>
</xsd:complexType>
	
<xsd:complexType name="mineralLayerType">
	<xsd:sequence>
		<xsd:element name="Ground" type="xsd:boolean" minOccurs="0"/>
		<xsd:element name="Horizon" type="xsd:string" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Description</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="Volume" type="xsd:string" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>in Cubic Meter</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
	</xsd:sequence>
</xsd:complexType>

<xsd:complexType name="mineralSoilType">
	<xsd:sequence>
    <xsd:element name="DepthInterval" type="xsd:int" minOccurs="0">
      <xsd:annotation>
        <xsd:documentation>In Centimeter</xsd:documentation>
      </xsd:annotation>
    </xsd:element>
		<xsd:element name="Layer" type="mineralLayerType" minOccurs="0"/>
	</xsd:sequence>
</xsd:complexType>

<xsd:complexType name="depthRangeType">
	<xsd:sequence>
    <xsd:element name="Min" type="xsd:int" minOccurs="0">
      <xsd:annotation>
        <xsd:documentation>in Centimeter</xsd:documentation>
      </xsd:annotation>
    </xsd:element>
    <xsd:element name="Max" type="xsd:int"  minOccurs="0">
      <xsd:annotation>
        <xsd:documentation>in Centimeter</xsd:documentation>
      </xsd:annotation>
    </xsd:element>
	</xsd:sequence>
</xsd:complexType>

<xsd:complexType name="organicLayerType">
	<xsd:sequence>
		<xsd:element name="Information" type="xsd:string" minOccurs="0" />
		<xsd:element name="OrganicLayerType" type="organicLayerElementType" minOccurs="0" />
		<xsd:element name="Density" type="xsd:string" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>in g/cm**3</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
	</xsd:sequence>
</xsd:complexType>

	<!--*************************************************-->
<!-- Location -->
<!--*************************************************-->

	<xsd:complexType name="sampleLocationType">
		<xsd:sequence>
			<xsd:element name="Site" type="siteType" minOccurs="0"/>
			<xsd:element name="Plot" type="plotType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="siteType">
		<xsd:sequence>
			<xsd:element name="Name" type="xsd:string"/>
			<xsd:element name="Abbreviation" type="xsd:string" minOccurs="0"/>
			<xsd:element name="BoundingBox" type="boundingBoxType"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="plotType">
		<xsd:sequence>
			<xsd:element name="Name" type="xsd:string"/>
			<xsd:element name="Abbreviation" type="xsd:string" minOccurs="0"/>
			<xsd:element name="GroundwaterDepth" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Vegetation" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Coordinates" type="coordinatesType" minOccurs="0"/>
			<xsd:element name="BoundingBox" type="boundingBoxType"/>
		</xsd:sequence>
	</xsd:complexType>
	
	
	<xsd:complexType name="coordinatesType">
		<xsd:sequence>
			<xsd:element name="Longtitude" type="xsd:string"/>
			<xsd:element name="Laditude" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="boundingBoxType">
		<xsd:sequence>
			<xsd:element name="EasternLongtiude" type="xsd:string" minOccurs="0"/>
			<xsd:element name="WesternLongtiude" type="xsd:string" minOccurs="0"/>
			<xsd:element name="NorthernLaditude" type="xsd:string" minOccurs="0"/>
			<xsd:element name="SouthernLaditude" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>

