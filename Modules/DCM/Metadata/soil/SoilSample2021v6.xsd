<?xml version="1.0" encoding="utf-8"?>
<xsd:schema targetNamespace="http://tempuri.org/XMLSchema.xsdd"
		elementFormDefault="qualified"
		xmlns="http://tempuri.org/XMLSchema.xsdd"
		xmlns:mstns="http://tempuri.org/XMLSchema.xsdd"
		xmlns:xsd="http://www.w3.org/2001/XMLSchema"
>
<xsd:element name="Metadata">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Contact" type="personType">
					<xsd:annotation>
						<xsd:documentation>Contact of the sample </xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Ownership" type="ownershipType" />
				<xsd:element name="SampleLocation" type="sampleLocationType" />
				<xsd:element name="SampleType" type="soilSampleType" >
					<xsd:annotation>
						<xsd:documentation>Soil</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Archive" type="archivedType" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
</xsd:element>

<xsd:complexType name="ownershipType">
	<xsd:sequence>
		<xsd:element name="Owner" type="ownerType" maxOccurs="unbounded">
			<xsd:annotation>
			<xsd:documentation>The owner of the sample.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="Organisation" type="organisationType" minOccurs="0" />
		<xsd:element name="Institute" type="instituteType" minOccurs="0" />
		<xsd:element name="Project" type="projectType" minOccurs="0" />
	</xsd:sequence>
</xsd:complexType>
 
<xsd:complexType name="soilSampleType">
	<xsd:sequence>
    <xsd:element name="Name" type="xsd:string" />
    <xsd:element name="Type" type="type" minOccurs="0" />
    <xsd:element name="SamplingType" type="soilSamplingType" minOccurs="0">
      <xsd:annotation>
        <xsd:documentation>How the soil sample was taken?</xsd:documentation>
      </xsd:annotation>
    </xsd:element>
    <xsd:element name="SampleType" type="soilElementType" maxOccurs="unbounded" minOccurs="1" />
		<xsd:element name="Description" type="xsd:string" minOccurs="0" />
		<xsd:element name="Comments" type="xsd:string" minOccurs="0" />
    <xsd:element name="Sieved" type="xsd:boolean" minOccurs="0"/>
    <xsd:element name="Ground" type="xsd:boolean" minOccurs="0"/>
		<xsd:element name="AirDried" type="xsd:boolean" minOccurs="0"/>
		<xsd:element name="DriedAtTemperature" type="xsd:double" minOccurs="0"/>
		<xsd:element name="SamplingDate" type="xsd:dateTime" minOccurs="0" />
		<xsd:element name="Vegetation" type="xsd:string" minOccurs="0"/>
		<xsd:element name="Status" type="xsd:string" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Enter the state of the sample here.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="MixedSample" type="xsd:boolean" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Put a check if the sample is not a single sample.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="PitSize" type="xsd:double" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>in Cubic Meter</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="TotalDepth" type="xsd:double" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>in Meter</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="DepthRange" type="depthRangeType" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>in Centimeter</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="Weight" type="xsd:string" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>in Gramm</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="Soil" type="soilType2" minOccurs="0" maxOccurs="1" />
		
	</xsd:sequence>
</xsd:complexType>


<!--*************************************************-->
<!-- Complex Types-->
<!--*************************************************-->
<!-- Base -->
<!--*************************************************-->
<xsd:complexType name="personType" >
		<xsd:sequence>
			<xsd:element name="Name"  type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Name of a person.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Email" type="email" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation></xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Phone" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Phone of a person.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
</xsd:complexType>

	<xsd:complexType name="contactType" >
		<xsd:sequence>
			<xsd:element name="Name"  type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>The name of the dataset Manager. This should be the PI of your project.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Email" type="email" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Official email address of the PI.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Phone" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Official phone number of the PI.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="ownerType" >
	<xsd:sequence>
		<xsd:element name="Name"  type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>Name of a person.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="Email" type="email" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation></xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="Phone" type="xsd:string" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Phone of a person.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
	</xsd:sequence>
	</xsd:complexType>

<xsd:complexType name="instituteType">
		<xsd:sequence>
			<xsd:element name="Name"  type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Name of the institute.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Abbreviation"  type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A Abbreviation of the institute.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="URL" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>URL to the mainpage of the insitute.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Address" type="addressType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Location information</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
</xsd:complexType>

<xsd:complexType name="organisationType">
	<xsd:sequence>
		<xsd:element name="Name"  type="xsd:string">
			<xsd:annotation>
				<xsd:documentation></xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="Abbreviation"  type="xsd:string" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation></xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="URL"  type="xsd:string" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation></xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="Address" type="addressType" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Location information</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
	</xsd:sequence>
</xsd:complexType>

<xsd:complexType name="projectType">
	<xsd:sequence>
		<xsd:element name="Name"  type="xsd:string">
			<xsd:annotation>
				<xsd:documentation></xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="Acronym"  type="xsd:string" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation></xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="Funder"  type="xsd:string" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation></xsd:documentation>
			</xsd:annotation>
		</xsd:element>
	</xsd:sequence>
</xsd:complexType>

	<xsd:complexType name="addressType">
	<xsd:sequence>
		<xsd:element name="Street"  type="xsd:string"/>
		<xsd:element name="Zipcode"  type="xsd:string"/>
		<xsd:element name="City"  type="xsd:string"/>
		<xsd:element name="Country"  type="countryType"/>
	</xsd:sequence>
</xsd:complexType>

<xsd:complexType name="archivedType">
	<xsd:sequence>
		<xsd:element name="SampleIsArchived" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>The sample is archived?</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="Room" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>The room, where the sample is archived.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="Rack" type="xsd:int">
			<xsd:annotation>
				<xsd:documentation>The rack, where the sample is archived.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="Box" type="xsd:int">
			<xsd:annotation>
				<xsd:documentation>The box, where the sample is archived.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
	</xsd:sequence>
</xsd:complexType>

